 },
 "homepage": "https://github.com/DevArtist254/instaclone#readme",
 "dependencies": {
  "cors": "^2.8.5",
  "dotenv": "^16.0.3",
  "express": "4.18.1",
  "mongoose": "^6.6.4"
 },
 "engines": {
  "node": "18.8.0"
 },
 "devDependencies": {
  "concurrently": "^7.4.0",
  "nodemon": "^2.0.20"
 }
